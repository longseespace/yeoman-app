{
  "menu": [
    {
      "label": "&File",
      "submenu": [
        {
          "label": "&Options",
          "command": "application:show-settings"
        },
        {
          "type": "separator"
        },
        {
          "label": "E&xit",
          "command": "application:quit",
          "accelerator": "Alt+F4"
        }
      ]
    },
    {
      "label": "Edit",
      "submenu": [
        {
          "label": "Undo",
          "accelerator": "Control+Z",
          "role": "undo"
        },
        {
          "label": "Redo",
          "accelerator": "Shift+Control+Z",
          "role": "redo"
        },
        {
          "type": "separator"
        },
        {
          "label": "Cut",
          "accelerator": "Control+X",
          "role": "cut"
        },
        {
          "label": "Copy",
          "accelerator": "Control+C",
          "role": "copy"
        },
        {
          "label": "Paste",
          "accelerator": "Control+V",
          "role": "paste"
        },
        {
          "label": "Select All",
          "accelerator": "Control+A",
          "role": "selectall"
        }
      ]
    },
    {
      "label": "Navigate",
      "submenu": [
        {
          "label": "Home",
          "accelerator": "Shift+Control+H",
          "command": "navigate:home"
        },
        {
          "label": "Back",
          "accelerator": "Control+[",
          "command": "navigate:back"
        }
      ]
    },
    {
      "label": "&View",
      "submenu": [
        {
          "label": "&Reload",
          "command": "window:reload",
          "accelerator": "Control+R"
        },
        {
          "label": "Toggle &Full Screen",
          "command": "window:toggle-full-screen",
          "accelerator": "Control+Shift+F"
        },
        {
          "label": "Developer",
          "submenu": [
            {
              "label": "Run Tests...",
              "command": "application:run-specs",
              "accelerator": "Control+Shift+T"
            },
            {
              "label": "Toggle Developer &Tools",
              "command": "window:toggle-dev-tools",
              "accelerator": "Control+Shift+I"
            }
          ]
        }
      ]
    },
    {
      "label": "&Help",
      "submenu": [
        {
          "label": "Report Issue",
          "command": "application:report-issue"
        },
        {
          "label": "About <%= productName %>",
          "command": "application:about"
        }
      ]
    }
  ]
}
